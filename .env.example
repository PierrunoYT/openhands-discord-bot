# Discord Bot Token (get from https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_bot_token_here

# Context7 API Key (get from https://context7.com/dashboard)
# Optional — the bot works without it but with lower rate limits
CONTEXT7_API_KEY=your_context7_api_key_here

# OpenHands Cloud API (get from https://app.all-hands.dev/settings → API Keys)
OPENHANDS_API_KEY=your_openhands_api_key_here
OPENHANDS_BASE_URL=https://app.all-hands.dev/api
GITHUB_REPO=username/repository
